#include <bits/stdc++.h> 
int lcs(string &str1, string &str2){
    int n=str1.size();
    int m=str2.size();
    vector<vector<int>> dp(n+1,vector<int>(m+1,0));
    
    for(int i=0;i<=n;i++)
    {
    dp[i][0]=0;
    }
    
    for(int i=0;i<=m;i++)
    {
    dp[0][i]=0;
    }
    int ans=0;
    for(int ind1=1;ind1<=n;ind1++)
    {
    for(int ind2=1;ind2<=m;ind2++)
    {
    if(str1[ind1-1]==str2[ind2-1])
    {
    dp[ind1][ind2]=1+dp[ind1-1][ind2-1]; //if previous ones are same as well
     ans=max(ans,dp[ind1][ind2]);
    }
    else
        {
    dp[ind1][ind2]=0;
        }
    }
    }
    return ans;
}
